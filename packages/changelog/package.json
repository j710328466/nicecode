{
  "name": "@nicecode/changelog",
  "version": "0.3.1",
  "author": "nicecoders",
  "description": "changelog",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "cz": "git add . && git cz",
    "log": "conventional-changelog --config src/log -i CHANGELOG.md -s -r 0",
    "v": "standard-version -r ${v}"
  },
  "keywords": [
    "changelog",
    "git"
  ],
  "config": {
    "commitizen": {
      "path": "./src/cz"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "compare-func": "^1.3.2",
    "longest": "^2.0.1",
    "q": "^1.5.1",
    "right-pad": "^1.0.1",
    "work-wrap": "^1.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "github-url-from-git": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicecoders/nicecode"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://nicecoders.github.io/nicecode/#/changelog",
  "gitHead": "6586b5a71b6af55ae6317c8979b177a4725bbc8d"
}
